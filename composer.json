{
    "name": "dframe/dframe-demo",
    "description": "It is a simple demo website based on Dframe Framework",
    "license": "MIT",
    "keywords": [
        "dframe",
        "demo",
        "framework",
        "website",
        "application",
        "microservices"
    ],
    "homepage": "https://dframeframework.com",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "app/"
        }
    },
    "require": {
        "dframe/dframe": "^v4.1",
        "dframe/database": "^v2.0",
        "smarty/smarty": "^3.1",
        "dframe/mymail": "^2.0",
        "php": ">=7.3",
        "ext-pdo": "*",
        "ext-json": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "Dframe\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": [
            "@phpunit"
        ],
        "phpunit": "php vendor/bin/phpunit"
    }
}
